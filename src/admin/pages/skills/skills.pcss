@import "../../../styles/mixins.pcss";

.about {
  &__list {
    display: flex;
    flex-wrap: wrap;
    margin-left: -30px;

    @include tablets {
      flex-direction: column;
    }
  }

  &__item {
    flex-basis: calc(50% - 30px);
    margin-left: 30px;
    margin-bottom: 30px;
    box-shadow: 0 0 5px 0px;
  }
}